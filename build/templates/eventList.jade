table
	thead
		tr
			th Event Type
			th Time
			th Actor
			th Repository
			th Public
	tbody
		each item in items
			tr
				td
					div.tweleve.columns
						=item.type
				td
					div.tweleve.columns
						=item.created_at
				td
					div.tweleve.columns
						a(href=item.actor.url)
							=item.actor.login
				td
					div.tweleve.columns
						a(href=item.repo.url)
							=item.repo.name
				td
					div.six.columns
						if (item.public)
							b(style="color:green")| Yes
						else
							b(style="color:red")| No